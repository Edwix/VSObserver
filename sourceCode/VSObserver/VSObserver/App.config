<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <!--<startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0"/>
  </startup>-->
  <appSettings>
    <!--IP of RTC server-->
    <add key="IpAddrRTC" value="10.23.154.180"/>

    <!--SQL lite database-->
    <add key="PathDataBase" value="C:\GIT\PRASA-Deliveries\UTest_bin\PRASA_LV.gc_UTEST1"/>
    
    <!--Refresh rate is in milliseconds-->
    <add key="RefreshRate" value="150"/>

    <!--Color when a value has changed (Bytes) value beetween 0 to 255
        Crystal Blue = R:161, G:226, B:255-->
    <add key="RedValue" value="161"/>
    <add key="GreenValue" value="226"/>
    <add key="BlueValue" value="255"/>

    <!--Time that colors stay on. Time is in seconds-->
    <add key="DurationColor" value="2"/>

    <!--Number of visble variable in dashboard-->
    <add key="NumberVisibleVariable" value="100"/>

    <!--Path of file to add the rules-->
    <add key="PathRuleFile" value="PRASA-VariablesRules.xml"/>

    <!--All colours for the choice in the rules
    the key must start by ColRule(Colour)-->
    <add key="ColRuleAqua" value="Aqua" />
    <add key="ColRuleBlue" value="Blue" />
    <add key="ColRuleChocolate" value="Chocolate" />
    <add key="ColRuleCrimson" value="Crimson" />
    <add key="ColRuleDarkOrchid" value="DarkOrchid" />
    <add key="ColRuleDarkRed" value="DarkRed" />
    <add key="ColRuleDodgerBlue" value="DodgerBlue" />
    <add key="ColRuleGold" value="Gold" />
    <add key="ColRuleGreen" value="#CECECE" />
    <add key="ColRuleGray" value="Gray" />
    <add key="ColRuleHotPink" value="HotPink" />
    <add key="ColRuleLimeGreen" value="LimeGreen" />
    <add key="ColRuleOrange" value="Orange" />
    <add key="ColRulePurple" value="Purple" />
    <add key="ColRuleRed" value="Red" />
    <add key="ColRuleYellow" value="Yellow" />
  </appSettings>
</configuration>