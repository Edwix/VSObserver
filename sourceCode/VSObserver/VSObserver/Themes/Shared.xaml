<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="WindowBackgroundColor" Color="#F7F7F7"/>
    <SolidColorBrush x:Key="BorderColor" Color="#F7F7F7"/>
    <SolidColorBrush x:Key="DarkBorderColor" Color="#8D8D8D"/>
    <SolidColorBrush x:Key="DarkErrorBorderColor" Color="#B40404"/>
    <SolidColorBrush x:Key="BackgroundErrorColor" Color="#F6CECE"/>
    <LinearGradientBrush x:Key="BackgroundError" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#FF0000" Offset="0" />
        <GradientStop Color="#FF3131" Offset="1" />
    </LinearGradientBrush>
</ResourceDictionary>